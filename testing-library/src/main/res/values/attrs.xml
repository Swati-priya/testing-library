        <attr name="zs_paddingBottom" format="dimension" />
        <attr name="zs_gravity" format="enum">
            <enum name="center" value="0" />
            <enum name="left" value="1" />
            <enum name="right" value="2" />
            <enum name="top" value="3" />
            <enum name="bottom" value="4" />
        </attr>
        <attr name="zs_fontFamily" format="reference" />
